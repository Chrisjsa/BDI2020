CREATE TABLE IF NOT EXISTS `arma_fiesta`.`entidad` (
  `id_entidad` INT NOT NULL,
  `atributte` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`id_entidad`),
  INDEX `fk_entidad_entidad_idx` (`fk_entidad` ASC),
  CONSTRAINT `fk_entidad_entidad`
    FOREIGN KEY (`fk_entidad`)
    REFERENCES `arma_tu_fiesta`.`entidad` (`id_entidad`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB



